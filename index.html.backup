            'gyn-bg-stat-tan-light': '#fde8c5',
            'gyn-text-positive-light': '#166534',
            'gyn-text-negative-light': '#991b1b',

            // Dark Theme Colors - Modern Refresh
            'gyn-bg-primary-dark': '#0f172a',    // slate-900
            'gyn-bg-secondary-dark': '#1e293b',  // slate-800
            'gyn-bg-tertiary-dark': '#334152',   // slate-700
            'gyn-text-primary-dark': '#cbd5e1',   // slate-300
            'gyn-text-secondary-dark': '#f1f5f9',  // slate-100
            'gyn-border-primary-dark': '#334152',   // slate-700
            'gyn-border-secondary-dark': '#475569',  // slate-600
            'gyn-accent-dark': '#f5a623',        // amber-500 (Kept)
            'gyn-sidebar-dark': '#0f172a',      // slate-900
            'gyn-sidebar-text-dark': '#f1f5f9',    // slate-100
            'gyn-sidebar-interactive-dark': '#1e293b', // slate-800
            'gyn-bg-error-dark': '#7f1d1d',      // red-900
            'gyn-border-error-dark': '#ef4444',   // red-500
            'gyn-text-error-dark': '#fecaca',     // red-200
            'gyn-bg-warning-dark': '#7c2d12',    // orange-900
            'gyn-border-warning-dark': '#f97316', // orange-500
            'gyn-text-warning-dark': '#fed7aa',   // orange-200
